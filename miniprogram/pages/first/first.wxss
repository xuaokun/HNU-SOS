page{
  width: 100%;
  height: 100%;
}
.container{
  width: 100%;
  height: 100%;
  position: relative;
}
.enter{
  position: absolute;
  opacity: 0.8;
  background-color: #fff;
  bottom: 300rpx;
  animation: entertext 2s;
}
@keyframes entertext {
  0% {
    opacity: 0;
    bottom: -100rpx;
  }
  100% {
    opacity: 1;
    background-color: #fff;
    bottom: 300rpx;
  }
}
